    <bannerLeft>
        <name>
            <![CDATA[
             <span style="color:black">Force Field X</span>
             <br>
             <h4>
             <span style="color:green">Sustainable</span><span style="color:black"> Software for Molecular Biophysics</span>
             </h4>
            ]]>
        </name>
        <href>http://ffx.biochem.uiowa.edu</href>
    </bannerLeft>
    <bannerRight>
        <name>The University of Iowa</name>
        <src>/images/UofI.png</src>
        <href>http://www.uiowa.edu</href>
        <width>100px</width>
        <height>100px</height>
    </bannerRight>
    <custom>
        <fluidoSkin>
            <!-- leftColumnClass (span2) plus bodyColumnClass (span10) should sum to 12 -->
            <leftColumnClass>span2</leftColumnClass>
            <bodyColumnClass>span10</bodyColumnClass>
            <sideBarEnabled>true</sideBarEnabled>
            <topBarEnabled>true</topBarEnabled>
            <topBarContainerStyle>width: 90%;</topBarContainerStyle>
            <topBarIcon>
                <name>Force Field X</name>
                <alt>Force Field X</alt>
                <src>/images/icon90.png</src>
                <href>/index.html</href>
            </topBarIcon>
            <navBarStyle>navbar-inverse</navBarStyle>
            <googleSearch>
                <sitesearch/>
            </googleSearch>
            <sourceLineNumbersEnabled>false</sourceLineNumbersEnabled>
            <copyrightClass>pull-right</copyrightClass>
            <gitHub>
                <projectId>mjschnie/ffx</projectId>
                <!-- ribbonOrientation: left/right -->
                <ribbonOrientation>right</ribbonOrientation>
                <!-- ribbonColor: red/green/black/darkblue/orange/gray -->
                <ribbonColor>green</ribbonColor>
            </gitHub>
            <!-- profile: pre-release|sandbox|retired -->
            <profile>production</profile>
            <!-- ohloh>
                <projectId>487436</projectId>
                <widget>factoids</widget>
            </ohloh -->
            <!-- default is a forward / -->
            <breadcrumbDivider>Â»</breadcrumbDivider>
        </fluidoSkin>
    </custom>

