parameters dimethylphosphate.prm

# From the end of equilibration
a-axis 36.323827531807700

# Linear charging from L=0 to L=1.0
elec-lambdaterm true 
# For OpenMM
elec-start                0.0
permanent-lambda-start    0.0
permanent-lambda-exponent 1.0

# Do not apply lambda to vdW
vdw-lambdaterm  false 

# Use OpenMM
platform        omm

heavy-hydrogen true
scf-algorithm   ept

