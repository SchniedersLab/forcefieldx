                &lt;/mmCIF:phasing_MIR_der_shell&gt;
                &lt;mmCIF:phasing_MIR_der_shell der_id=&quot;KAu(CN)2&quot; d_res_low=&quot;6.4&quot; d_res_high=&quot;5.2&quot;&gt;
