                                            <xsd:minExclusive value="0.0"/>
                                            <xsd:maxExclusive value="10.0"/>
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:decimal">
