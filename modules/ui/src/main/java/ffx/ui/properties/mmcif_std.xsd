                                4
                                7th symm. posn.; +a on x; -b on y
                                7_645
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="geom_contactType">
