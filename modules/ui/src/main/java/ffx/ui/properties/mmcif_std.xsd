                                    of attribute value_angle in category chem_comp_angle.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:decimal">
