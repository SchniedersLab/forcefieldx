                &lt;mmCIF:struct_mon_prot_cisCategory&gt;
                &lt;mmCIF:struct_mon_prot_cis label_comp_id=&quot;PRO&quot; label_seq_id=&quot;8&quot; label_asym_id=&quot;L&quot;
