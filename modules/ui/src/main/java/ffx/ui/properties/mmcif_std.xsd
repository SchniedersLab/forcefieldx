                                            <xsd:maxExclusive value="180.0"/>
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                        <xsd:restriction base="xsd:decimal">
                                            <xsd:minInclusive value="0.0"/>
