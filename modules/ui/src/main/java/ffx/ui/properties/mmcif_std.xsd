                label_alt_id=&quot;&quot;&gt;&lt;/mmCIF:struct_mon_prot_cis&gt;
                &lt;mmCIF:struct_mon_prot_cis label_comp_id=&quot;PRO&quot; label_seq_id=&quot;95&quot; label_asym_id=&quot;L&quot;
