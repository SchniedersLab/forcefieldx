                USA&lt;/PDBx:address&gt;
   &lt;/PDBx:pdbx_audit_author&gt;
&lt;/PDBx:pdbx_audit_authorCategory&gt;


            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="pdbx_audit_author" minOccurs="0"  maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:all>
