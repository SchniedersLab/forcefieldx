                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:simpleContent>
                                    <xsd:extension base="xsd:decimal">
                                        <xsd:attribute fixed="angstroms" name="units" type="xsd:string" use="optional" />
                                    </xsd:extension>
                                </xsd:simpleContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Cartn_x" minOccurs="0" maxOccurs="1" nillable="true" >
                            <xsd:annotation>
                                <xsd:documentation xml:lang="en" >
